# Network alias
db_alias="db"
redis_alias="redis"
api_alias="api"
# MySQL Default PW
db_pw="password1234!"
# Restart option
restart_option="unless-stopped"
# Container Name
CNAME_DB="board-api-db"
CNAME_REDIS="board-api-redis"
CNAME_API="board-api"
