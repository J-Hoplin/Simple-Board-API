# Port Config
DB_PORT="3600"
REDIS_PORT="6379"
API_PORT1="4000"
API_PORT2="6000"
# network, volume name
NETWORK_NAME="api-network"
VOLUME_NAME="board-api-volume"
# mysql image name
DB_IMAGE="mysql"
